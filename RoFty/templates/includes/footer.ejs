<div class="site-footer">
  <div class="container">
    <div class="row">
      <div class="col-lg-7">
        <div class="detial">
          <%- themeConfig.footerInfo %>
        </div>
      </div>
      <div class="col-lg-5">
        <div class="about-me">
          <div class="avator"
            style="background: url('<%= themeConfig.domain %>/images/avatar.png?v=<%= site.utils.now %>');background-size: cover;">
            <div class="mask-dark"></div>
          </div>
          <div class="social-link">
            <% ['github', 'twitter' , 'weibo' , 'zhihu' , 'facebook' , 'wechat' , 'qq' ].forEach((item)=> { %>
              <% if (site.customConfig[item]) { %>
                <a href="<%= site.customConfig[item] %>" class="clearfix" target="_blank">
                  <i class="ri-<%= item %>-line"></i>
                </a>
                <% } %>
                  <% }) %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>